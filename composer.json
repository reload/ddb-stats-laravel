{
    "name": "danskernesdigitalebibliotek/ddb-stats-laravel",
    "description": "Common stats collecting framework for Lumen based DDB microservices",
    "type": "library",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "laravel/lumen-framework": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "DDB\\Stats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DDB\\Stats\\": "tests/"
        }
    },
    "license": "AGPL",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^1.0",
        "nunomaduro/larastan": "^1.0.0",
        "orchestra/testbench": "^5.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^1.3",
        "phpstan/phpstan-mockery": "^1.0",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^5.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DDB\\Stats\\StatisticsServiceProvider"
            ]
        }
    }
}
