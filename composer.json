{
    "name": "danskernesdigitalebibliotek/ddb-stats-laravel",
    "description": "Common stats collecting framework for Lumen based DDB microservices",
    "type": "library",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/database": "^6.0",
        "illuminate/support": "^6.0",
        "phpunit/phpcov": "^6.0",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "DDB\\Stats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DDB\\Stats\\": "tests/"
        }
    },
    "license": "AGPL",
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "laravel/lumen-framework": "^6.0"
    }
}
